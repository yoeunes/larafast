{
    "name"             : "yoeunes/larafast",
    "license"          : "MIT",
    "description"      : "boilerplate package for rapid laravel development.",
    "homepage"         : "https://github.com/yoeunes/larafast",
    "authors"          : [
        {
            "name"    : "Younes Khoubza",
            "email"   : "younes.khoubza@gmail.com",
            "homepage": "https://dalilbook.com",
            "role"    : "Developer"
        }
    ],
    "require"          : {
        "php": ">=7.0.0",
        "barryvdh/laravel-cors": "^0.10.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "illuminate/database": "^5.5",
        "illuminate/filesystem": "^5.5",
        "illuminate/http": "^5.5",
        "illuminate/routing": "^5.5",
        "illuminate/support": "^5.5",
        "illuminate/view": "^5.5",
        "laravel/passport": "^4.0",
        "laravelcollective/html": "^5.5",
        "oriceon/toastr-5-laravel": "dev-master",
        "spatie/laravel-fractal": "^5.3",
        "spatie/laravel-medialibrary": "^6.6",
        "spatie/laravel-permission": "^2.7",
        "yajra/laravel-datatables": "^1.0"
    },
    "require-dev"      : {
        "mockery/mockery"    : "^1.0",
        "phpunit/phpunit"    : "^6.5",
        "orchestra/testbench": "^3.5",
        "laracasts/TestDummy": "^2.3"
    },
    "autoload"         : {
        "psr-4": {
            "Yoeunes\\Larafast\\": "src"
        }
    },
    "autoload-dev"     : {
        "psr-4": {
            "Yoeunes\\Larafast\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "extra"            : {
        "laravel": {
            "providers": [
                "Barryvdh\\Cors\\ServiceProvider",
                "Laravel\\Passport\\PassportServiceProvider",
                "Collective\\Html\\HtmlServiceProvider",
                "Spatie\\Fractal\\FractalServiceProvider",
                "Spatie\\MediaLibrary\\MediaLibraryServiceProvider",
                "Spatie\\Permission\\PermissionServiceProvider",
                "Yajra\\DataTables\\DataTablesServiceProvider",
                "Yoeunes\\Larafast\\LarafastServiceProvider"
            ],
            "aliases"  : {
                "Form"   : "Collective\\Html\\FormFacade",
                "Html"   : "Collective\\Html\\HtmlFacade",
                "Fractal": "Spatie\\Fractal\\FractalFacade"
            }
        }
    },
    "config"           : {
        "sort-packages": true
    }
}
