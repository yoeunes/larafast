{
    "name"             : "yoeunes/larafast",
    "license"          : "MIT",
    "description"      : "boilerplate package for rapid laravel development.",
    "homepage"         : "https://github.com/yoeunes/larafast",
    "authors"          : [
        {
            "name"    : "Younes Khoubza",
            "email"   : "younes.khoubza@gmail.com",
            "homepage": "https://dalilbook.com",
            "role"    : "Developer"
        }
    ],
    "require"          : {
        "php": ">=7.0.0",
        "barryvdh/laravel-cors": "^0.11",
        "barryvdh/laravel-ide-helper": "^2.4",
        "illuminate/database": "^5.5",
        "illuminate/filesystem": "^5.5",
        "illuminate/http": "^5.5",
        "illuminate/routing": "^5.5",
        "illuminate/support": "^5.5",
        "illuminate/view": "^5.5",
        "laravel/passport": "^4.0",
        "laravelcollective/html": "^5.5",
        "spatie/laravel-fractal": "^5.3",
        "spatie/laravel-medialibrary": "^6.6",
        "spatie/laravel-permission": "^2.7",
        "yajra/laravel-datatables": "^1.0",
        "yoeunes/toastr": "^1.0"
    },
    "require-dev"      : {
        "mockery/mockery"    : "^1.0",
        "phpunit/phpunit"    : "^6.5",
        "orchestra/testbench": "^3.5",
        "laracasts/TestDummy": "^2.3"
    },
    "autoload"         : {
        "psr-4": {
            "Yoeunes\\Larafast\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev"     : {
        "psr-4": {
            "Yoeunes\\Larafast\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "extra"            : {
        "laravel": {
            "providers": [
                "Yoeunes\\Larafast\\LarafastServiceProvider"
            ]
        }
    },
    "config"           : {
        "sort-packages": true
    }
}
