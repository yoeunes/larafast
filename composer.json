{
    "name"             : "yoeunes/larafast",
    "license"          : "MIT",
    "description"      : "boilerplate package for rapid laravel development.",
    "homepage"         : "https://github.com/yoeunes/larafast",
    "keywords"         : [
        "larafast",
        "toastr",
        "boilerplate",
        "laravel",
        "laravel 5",
        "php"
    ],
    "authors"          : [
        {
            "name"    : "Younes Khoubza",
            "email"   : "younes.khoubza@gmail.com",
            "homepage": "https://dalilbook.com",
            "role"    : "Developer"
        }
    ],
    "require"          : {
        "php": ">=7.1.3",
        "barryvdh/laravel-ide-helper": "^2.4",
        "illuminate/database": "5.6.*",
        "illuminate/filesystem": "5.6.*",
        "illuminate/http": "5.6.*",
        "illuminate/routing": "5.6.*",
        "illuminate/support": "5.6.*",
        "illuminate/view": "5.6.*",
        "laravel/passport": "^6.0",
        "laravelcollective/html": "^5.6",
        "spatie/laravel-cors": "^1.2",
        "spatie/laravel-fractal": "^5.4",
        "spatie/laravel-medialibrary": "^7.3",
        "spatie/laravel-permission": "^2.9",
        "yajra/laravel-datatables": "^1.0",
        "yoeunes/toastr": "^1.0"
    },
    "require-dev"      : {
        "mockery/mockery"    : "^1.0",
        "phpunit/phpunit"    : "^7.1",
        "orchestra/testbench": "^3.6",
        "laracasts/TestDummy": "^2.3"
    },
    "autoload"         : {
        "psr-4": {
            "Yoeunes\\Larafast\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev"     : {
        "psr-4": {
            "Yoeunes\\Larafast\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "extra"            : {
        "laravel": {
            "providers": [
                "Yoeunes\\Larafast\\LarafastServiceProvider"
            ]
        }
    },
    "config"           : {
        "sort-packages": true
    }
}
